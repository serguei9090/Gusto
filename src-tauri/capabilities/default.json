{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "log:default",
    "fs:default",
    "fs:allow-write-text-file",
    "fs:allow-read-text-file",
    "dialog:default",
    "dialog:allow-save",
    "dialog:allow-open",
    "dialog:allow-message"
  ]
}
